{
  "name": "qbo-quantum-child-theme",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QbitOne/quantum-child.git"
  },
  "keywords": [
    "QbitOne",
    "WordPress",
    "Theme",
    "Child-Theme"
  ],
  "author": "Andreas Geyer, Michael Geyer",
  "bugs": {
    "url": "https://github.com/QbitOne/quantum-child/issues"
  },
  "homepage": "https://github.com/QbitOne/quantum-child#readme",
  "scripts": {
    "serve": "gulp serve ",
    "compile:css": "gulp compilecss",
    "compile:js": "gulp compilejs",
    "lint:css": "wp-scripts lint-style './postcss/*.css'",
    "lint:js": "wp-scripts lint-js './assets/js/*.js'",
    "bundle": "dir-archiver --src . --dest ../qbo-quantum-theme.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist gulpfile.js style"
  },
  "devDependencies": {
    "@wordpress/scripts": "^16.1.0",
    "autoprefixer": "^10.2.6",
    "browser-sync": "^2.26.14",
    "dir-archiver": "^1.2.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^9.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-terser": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "postcss": "^8.2.2",
    "postcss-discard-comments": "^4.0.2",
    "postcss-hexrgba": "^2.0.1",
    "postcss-import": "^14.0.0",
    "postcss-mixins": "^7.0.2",
    "postcss-nested": "^5.0.3",
    "postcss-preset-env": "^6.7.0",
    "postcss-simple-vars": "^6.0.2"
  }
